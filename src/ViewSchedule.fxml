<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<AnchorPane maxHeight="600.0" maxWidth="600.0" minHeight="600.0" minWidth="600.0" prefHeight="600.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="EmployerViewScheduleController">
   <children>
      <VBox alignment="CENTER" prefHeight="200.0" prefWidth="100.0">
         <children>
            <HBox maxHeight="500.0" maxWidth="600.0" minHeight="500.0" minWidth="600.0" prefHeight="500.0" prefWidth="600.0">
               <children>
                  <VBox alignment="CENTER" maxHeight="500.0" maxWidth="120.0" minHeight="500.0" minWidth="120.0" prefHeight="500.0" prefWidth="120.0">
                     <children>
                        <Text strokeType="OUTSIDE" strokeWidth="0.0" text="MONDAY" textAlignment="CENTER">
                           <font>
                              <Font name="Arial Bold" size="13.0" />
                           </font>
                        </Text>
                        <TextArea fx:id="monText" editable="false" maxHeight="400.0" minHeight="400.0" prefHeight="400.0" prefWidth="120.0" />
                     </children>
                  </VBox>
                  <VBox alignment="CENTER" maxHeight="500.0" maxWidth="120.0" minHeight="500.0" minWidth="120.0" prefHeight="500.0" prefWidth="120.0">
                     <children>
                        <Text strokeType="OUTSIDE" strokeWidth="0.0" text="TUESDAY" textAlignment="CENTER">
                           <font>
                              <Font name="Arial Bold" size="13.0" />
                           </font>
                        </Text>
                        <TextArea fx:id="tuesText" editable="false" maxHeight="400.0" minHeight="400.0" prefHeight="400.0" prefWidth="200.0" />
                     </children>
                  </VBox>
                  <VBox alignment="CENTER" maxHeight="500.0" maxWidth="120.0" minHeight="500.0" minWidth="120.0" prefHeight="500.0" prefWidth="120.0">
                     <children>
                        <Text strokeType="OUTSIDE" strokeWidth="0.0" text="WEDNESDAY" textAlignment="CENTER">
                           <font>
                              <Font name="Arial Bold" size="13.0" />
                           </font>
                        </Text>
                        <TextArea fx:id="wedText" editable="false" maxHeight="400.0" minHeight="400.0" prefHeight="400.0" prefWidth="200.0" />
                     </children>
                  </VBox>
                  <VBox alignment="CENTER" maxHeight="500.0" maxWidth="120.0" minHeight="500.0" minWidth="120.0" prefHeight="500.0" prefWidth="120.0">
                     <children>
                        <Text strokeType="OUTSIDE" strokeWidth="0.0" text="THURSDAY" textAlignment="CENTER">
                           <font>
                              <Font name="Arial Bold" size="13.0" />
                           </font>
                        </Text>
                        <TextArea fx:id="thursText" editable="false" maxHeight="400.0" minHeight="400.0" prefHeight="400.0" prefWidth="200.0" />
                     </children>
                  </VBox>
                  <VBox alignment="CENTER" maxHeight="500.0" maxWidth="120.0" minHeight="500.0" minWidth="120.0" prefHeight="500.0" prefWidth="120.0">
                     <children>
                        <Text strokeType="OUTSIDE" strokeWidth="0.0" text="FRIDAY" textAlignment="CENTER">
                           <font>
                              <Font name="Arial Bold" size="13.0" />
                           </font>
                        </Text>
                        <TextArea fx:id="friText" editable="false" maxHeight="400.0" minHeight="400.0" prefHeight="400.0" prefWidth="200.0" />
                     </children>
                  </VBox>
               </children>
            </HBox>
            <Button fx:id="returnButton" alignment="CENTER" mnemonicParsing="false" onAction="#handleReturn" text="Return" />
         </children>
      </VBox>
   </children>
</AnchorPane>
